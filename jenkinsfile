node {
  stage('Init') {
    checkout scm
  }

      stage('Deploy') {
        withCredentials([azureServicePrincipal('bb611361-dffa-47f6-a12d-01a82cc2186e')]) {
            sh 'az login --service-principal -u $AZURE_CLIENT_ID -p $AZURE_CLIENT_SECRET -t $AZURE_TENANT_ID'
            sh 'az account set -s $AZURE_SUBSCRIPTION_ID'
            sh 'az resource list'        
                    
        }
      }
}
